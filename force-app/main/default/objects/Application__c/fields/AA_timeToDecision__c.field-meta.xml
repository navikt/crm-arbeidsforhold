<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AA_timeToDecision__c</fullName>
    <description>Counts days from application submitted(new) to desicion(closed)</description>
    <externalId>false</externalId>
    <formula>if(isBlank(DecisionDate__c ),&apos;Ingen vedtak&apos;,TEXT(DecisionDate__c - ApplicationSubmittedDate__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Time to decision</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
